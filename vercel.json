{
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/config.js", "dest": "/api/config.js" },
    { "src": "/callback", "dest": "/api/index.js" },
    { "src": "/events", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
  }
  