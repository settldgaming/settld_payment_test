# Environment variables for Settld frontend
# Copy this file to `.env` and fill in your values before deployment.

# Port for running the server (ignored on Vercel deployments)
PORT=3000

# Base URL for the Settld API
SETTLD_API_BASE_URL=

# Base URL for linking to transactions on a block explorer
ETHERSCAN_TX_URL=https://sepolia.etherscan.io/tx/